<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <!-- 制定mysql的驱动包的路径 千万别放中文路径下 -->
    <classPathEntry location="D:\mysql.jar"/>
    <!-- 配置数据源和生成的代码所存放的位置 -->
    <context id="context1">
        <commentGenerator>
            <!-- 去除自动生成的注释 -->
            <property name="suppressAllComments" value="true"/>
            <!-- 是否去除所有自动生成的文件的时间戳，默认为false -->
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <!-- 数据源 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://localhost:3306/litte"
        userId="root"
        password="root" />
        <!--<jdbcConnection driverClass="oracle.jdbc.OracleDriver"
                        connectionURL="jdbc:oracle:thin:@172.19.116.149:1521/XYDYPDB02"
                        userId="xydy_corporation"
                        password="123456"/>-->

        <!-- 所生成的bean的位置默认资源包src E:\ztgj\CreditDYServer\src\main\java\com\jusfoun\domain\jncredit -->
        <javaModelGenerator targetPackage="com.litte.entity.reception"
                            targetProject="litte"/>
        <!-- 所生成的xml的影射文件的位置,默认资源包src -->
        <sqlMapGenerator targetPackage="mapper.reception"
                         targetProject="litte"/>
        <!-- 生成Dao类存放位置 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.litte.mapper.reception"
                             targetProject="litte"/>
        <!-- 为哪些表生成代码 tableName:表名 schema:不用填写,其余属性是禁用例子查询的生成 -->
        <table schema="" tableName="t_recruit" enableCountByExample="false"
               enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="true" selectByExampleQueryId="false"
        >
        </table>
    </context>
</generatorConfiguration>
